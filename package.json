{
  "name": "kea-valley-auto-minter",
  "version": "1.0.0",
  "description": "KEA Valley Token Auto-Minter",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:amoy": "NETWORK=amoy node src/index.js",
    "start:polygon": "NETWORK=polygon node src/index.js",
    "build:profile": "esbuild src/profile-app/index.jsx --bundle --outfile=src/public/profile/bundle.js --loader:.jsx=jsx",
    "build:login": "esbuild src/login-app/index.jsx --bundle --outfile=src/public/login/bundle.js --loader:.jsx=jsx",
    "build": "npm run build:profile && npm run build:login"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "pg": "^8.17.2",
    "stripe": "^20.2.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.0"
  }
}
